#include<stdio.h>
#include<conio.h>
void Swap(char *a,char *b,int n)
{
	char c;
	int i=0;
	for(i=0;i<10;i++)
	{
		c=a[i];
		a[i]=b[i];
		b[i]=c;
	}
	printf("Strings Before swaping\n");
	printf("%s \t %s",a,b);
}
int main()
{
	char a[10],b[10];
	int i,j;
	printf("Enter the strings");
	scanf("%s %s",&a,&b);
	printf("Strings Before swaping");
	printf("%s\n %s",a,b);
	Swap(a,b,10);
	return 0;
	getch();
	
}
