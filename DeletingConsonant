#include<stdio.h>
#include<conio.h>
int main()
{
	char str[10];
	int i=0,j=0;
	printf("Enter the string");
	scanf("%s",&str);
	for(i=0;i<10;i++)
	{
		if(str[i]=='a' || str[i]=='e' ||str[i]=='i' ||str[i]=='o' ||str[i]=='u')
		{
			str[j++]=str[i];	
		}
	}
	str[j]='\0';
	printf("%s",str);
	return 0;
	getch();
}
